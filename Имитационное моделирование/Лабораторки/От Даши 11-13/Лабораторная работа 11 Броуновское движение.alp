					<Action><![CDATA[Ball b = main.checkHit( this );
double rx = b.x - x;
double ry = b.y - y;
double vrx = vx - b.vx;
double vry = vy - b.vy;
double k = (vrx*rx+vry*ry) / (rx*rx+ry*ry);
double vnx = rx*k;
double vny = ry*k;
double vtx = vx - vnx;
double vty = vy - vny;
vx -= vnx;
vy -= vny;
b.vx += vnx;
b.vy += vny;
bounceBall.restart();]]></Action>
