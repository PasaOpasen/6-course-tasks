					<Parameter>
						<Name><![CDATA[b]]></Name>
						<Type><![CDATA[Ball]]></Type>
					</Parameter>
					<Body><![CDATA[for (Ball b1 : balls) {
	if (b1 == b)
		continue;
	double dx = b1.x - b.x;
	double dy = b1.y - b.y;
	// check the distance less than 2*r
	if (dx*dx+dy*dy < 4*b.r*b.r) {
		double vrx = b1.vx - b.vx;
		double vry = b1.vy - b.vy;
		// check the balls are moving towards
		if (dx*vrx + dy*vry <= 0)
			return b1;
	}
}
return null;]]></Body>
				</Function>
